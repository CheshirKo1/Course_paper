<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="line" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">3sp</item>
        <item name="android:background">#3ca389</item>
    </style>

    <style name="PositiveButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:textColor">@color/baseRed</item>
    </style>

    <style name="TextWithBlurShadowStyle">
        <item name="android:shadowColor">@color/baseRed</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">10</item>
    </style>

</resources>